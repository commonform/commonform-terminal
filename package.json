{
  "name": "commonform-terminal",
  "description": "display Common Form projects at text terminals",
  "version": "0.2.0",
  "author": {
    "name": "Kyle E. Mitchell",
    "email": "kyle@kemitchell.com",
    "url": "http://kemitchell.com/"
  },
  "bugs": {
    "url": "https://github.com/commonform/commonform-terminal/issues"
  },
  "dependencies": {
    "chalk": "^0.5.1",
    "commonform-decimal-numbers": "^0.1.1",
    "commonform-flatten": "^0.2.0",
    "commonform-number": "^0.2.1",
    "commonform-resolve": "^0.2.1"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "check-versions": "0.0.1",
    "istanbul": "^0.3.5",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "mocha": "^2.1.0"
  },
  "homepage": "https://github.com/commonform/commonform-terminal",
  "keywords": [
    "contracts",
    "document assembly",
    "knowledge management",
    "law"
  ],
  "license": "Apache-2.0",
  "main": "source/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/commonform/commonform-terminal.git"
  },
  "scripts": {
    "check": "check-versions --check-module",
    "coverage": "istanbul cover _mocha && istanbul check-coverage coverage/coverage.json",
    "lint": "jshint source test && jscs source test",
    "pre-commit": "npm run lint && npm run check && npm run coverage",
    "test": "mocha",
    "travis-ci": "npm run pre-commit"
  }
}
